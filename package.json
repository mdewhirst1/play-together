    "typescript": "^3.8.2"
  },
  "devDependencies": {
    "@types/node": "^12.7.0",
    "@types/sqlite3": "^3.1.6",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.3",
    "ts-node-dev": "^1.0.0-pre.44",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
